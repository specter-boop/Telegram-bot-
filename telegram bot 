import telebot

TOKEN = "BOT YOKEN"
bot = telebot.TeleBot(TOKEN)

@bot.message_handler(commands=['start'])
def start(message):
    bot.reply_to(message, "Selam dayÄ± ðŸ˜Ž Bot Ã§alÄ±ÅŸÄ±yor!")

@bot.message_handler(func=lambda m: True)
def echo(message):
    bot.reply_to(message, message.text)

bot.polling()
